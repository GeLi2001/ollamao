# ollamao Environment Configuration
# Copy this file to .env and customize for your deployment

# Server Configuration
OLLAMAO_HOST=0.0.0.0
OLLAMAO_PORT=8000
OLLAMAO_RELOAD=false

# Logging Configuration
OLLAMAO_LOG_LEVEL=INFO
OLLAMAO_LOG_FORMAT=json

# Development Mode
OLLAMAO_DEBUG=false

# Configuration Files
OLLAMAO_CONFIG_DIR=config
OLLAMAO_MODELS_CONFIG_FILE=models.yaml
OLLAMAO_KEYS_CONFIG_FILE=keys.yaml

# CORS Configuration (comma-separated)
OLLAMAO_CORS_ORIGINS=*
